<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coderdream.easyexcelpractise.mapper.CurveMapper">

    <resultMap type="com.coderdream.easyexcelpractise.entity.CurveEntity" id="CurveInfoMap">
        <result property="attrItemFullCode" column="attr_item_full_code" jdbcType="VARCHAR"/>
        <result property="value" column="value" jdbcType="OTHER" javaType="com.alibaba.fastjson.JSONArray" typeHandler="com.baomidou.mybatisplus.extension.handlers.FastjsonTypeHandler" />
    </resultMap>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into object_b_curve(`attr_item_full_code`, `value`)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.attrItemFullCode},
            #{entity.value})
        </foreach>
    </insert>

</mapper>

